generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Link {
  id          Int      @id @default(autoincrement())
  description String
  url         String
  createdAt   DateTime @default(now())
}
